<h1>This is my new resources page </h1>

<%= form_for @resource do |f| %>
  <p>
    <%= f.label :name %><br/>
    <%= f.text_field :name %>
  </p>
  
  <p>
    <%= f.label :type %><br/>
    <%= f.text_field :resource_type %>
  </p>
  
   <p>
    <%= f.label :description %><br/>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.submit %>
  </p>

<% end %>